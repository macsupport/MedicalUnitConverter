    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "cordova-res": "^0.15.4",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.31",
    "postcss-preset-env": "^8.4.2",
    "tailwindcss": "^3.3.3",
    "vite": "^4.3.9"
  }
}
