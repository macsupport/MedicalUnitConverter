{
  "name": "medical-converter",
  "private": true,
  "version": "1.4.0",
  "description": "Medical Converter",
  "repository": "",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vite build",
    "build-capacitor-ios": "cross-env NODE_ENV=production vite build && npx cap sync && npx cap copy ios",
    "build-cap": "cross-env NODE_ENV=production vite build && npx cap sync && npx cap copy",
    "build-capacitor-android": "cross-env NODE_ENV=production vite build && npx cap copy android",
    "postinstall": "cpy --flat ./node_modules/framework7-icons/fonts/*.* ./src/fonts/ && cpy --flat ./node_modules/material-icons/iconfont/*.* ./src/fonts/"
  },
  "browserslist": [
    "IOS >= 15",
    "Safari >= 15",
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "dependencies": {
    "@capacitor-community/electron": "^5.0.1",
    "@capacitor/android": "^7.2.0",
    "@capacitor/app": "^7.0.1",
    "@capacitor/app-launcher": "^7.0.1",
    "@capacitor/browser": "^7.0.1",
    "@capacitor/core": "^7.2.0",
    "@capacitor/filesystem": "^7.0.1",
    "@capacitor/ios": "^7.2.0",
    "@capacitor/keyboard": "^7.0.1",
    "@capacitor/share": "^7.0.1",
    "@capacitor/splash-screen": "^7.0.1",
    "@capawesome-team/capacitor-printer": "^7.1.0",
    "@capawesome/capacitor-app-review": "^7.0.1",
    "@fortawesome/fontawesome-pro": "^6.4.0",
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "api": "^6.0.0",
    "dom7": "^4.0.6",
    "framework7": "^8.1.0",
    "framework7-icons": "^5.0.5",
    "framework7-plugin-keypad": "^7.0.0",
    "framework7-svelte": "^8.1.0",
    "html2pdf.js": "^0.10.1",
    "jquery": "^3.7.0",
    "material-icons": "^1.13.8",
    "pdfmake": "^0.2.7",
    "shepherd.js": "^11.1.1",
    "skeleton-elements": "^4.0.1",
    "svelte": "^3.59.1",
    "svelte-infinite-scroll": "^2.0.1",
    "svelte-tiny-virtual-list": "^2.0.5",
    "swiper": "^9.4.0",
    "xml2js": "^0.6.0"
  },
